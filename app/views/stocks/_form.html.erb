 <%= form_for @stock do |f| %>
 	<%= render 'shared/error_messages', object: f.object %>
 	<fieldset>
	 	<%= f.label :food_id %>
	 	<%= f.collection_select :food_id, Food.all, :id, :name %>
		<%= f.label :expiration %>
		<%= f.text_field :expiration, :class => 'datepicker' %>

		<%= f.label :quantity %>
		<%= f.text_field :quantity %>
	</fieldset>
  	<%= f.submit 'Submit', :class => 'btn btn-primary' %>
 <% end %>
<script>
  $(function() {
  	$('.datepicker').datepicker({"format": "yyyy-mm-dd", "weekStart": 1, "autoclose": true});
  });
</script>